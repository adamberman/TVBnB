<div class="review-user">
	<p class="review-user-info"><%= user.escape('username') %></p>
</div>
<div class="review-info">
	<p class="review-info-content"><%= review.escape('content') %></p>
	<% var date = new Date(review.escape('created_at')) %>
	<p class="review-info-date"><%= date.getMonth() %>/<%= date.getDate() %>/<%= date.getFullYear() %></p>
</div>